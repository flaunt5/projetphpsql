<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Infrastructurelist" table="infrastructurelist">
    <indexes>
      <index name="clubId" columns="clubId"/>
      <index name="stableId" columns="stableId"/>
      <index name="infrastructureId" columns="infrastructureId"/>
    </indexes>
    <id name="idinfralist" type="integer" column="idInfraList">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="clubid" type="integer" column="clubId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="stableid" type="integer" column="stableId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="infrastructureid" type="integer" column="infrastructureId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
