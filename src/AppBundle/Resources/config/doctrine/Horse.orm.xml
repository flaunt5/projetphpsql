<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Horse" table="horse">
    <indexes>
      <index name="speciesId" columns="speciesId"/>
      <index name="stablesId" columns="stablesId"/>
    </indexes>
    <id name="idhorse" type="integer" column="idHorse">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="speciesid" type="integer" column="speciesId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="resistance" type="integer" column="resistance" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="stablesid" type="integer" column="stablesId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="endurance" type="integer" column="endurance" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="jumpability" type="integer" column="jumpAbility" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="speed" type="integer" column="speed" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="intelligence" type="integer" column="intelligence" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sociability" type="integer" column="sociability" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="temper" type="integer" column="temper" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="health" type="integer" column="health" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="moral" type="integer" column="moral" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="stress" type="integer" column="stress" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fatigue" type="integer" column="fatigue" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hunger" type="integer" column="hunger" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cleanliness" type="integer" column="cleanliness" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="experience" type="integer" column="experience" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="level" type="smallint" column="level" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="state" type="integer" column="state" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
