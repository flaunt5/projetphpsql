<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Bankoperations" table="bankoperations">
    <indexes>
      <index name="bankId" columns="bankId"/>
    </indexes>
    <id name="idbankop" type="integer" column="idBankOp">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="operationdatetime" type="datetime" column="operationDatetime" nullable="false"/>
    <field name="amount" type="integer" column="amount" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bankid" type="integer" column="bankId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
