<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Newspaper" table="newspaper">
    <indexes>
      <index name="userId" columns="userId"/>
      <index name="achievementId" columns="achievementId"/>
      <index name="weatherId" columns="weatherId"/>
    </indexes>
    <id name="idnewspaper" type="integer" column="idNewsPaper">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userid" type="integer" column="userId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="achievementid" type="integer" column="achievementId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="weatherid" type="integer" column="weatherId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
