<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Club" table="club">
    <indexes>
      <index name="ownerId" columns="ownerId"/>
      <index name="infrastructureListId" columns="infrastructureListId"/>
    </indexes>
    <id name="idclub" type="integer" column="idClub">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="capacity" type="boolean" column="capacity" nullable="false"/>
    <field name="ownerid" type="integer" column="ownerId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="infrastructurelistid" type="integer" column="infrastructureListId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="signupprice" type="decimal" column="signupPrice" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
