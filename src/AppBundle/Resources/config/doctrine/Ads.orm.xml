<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ads" table="ads">
    <id name="idads" type="integer" column="idAds">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="integer" column="name" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="content" type="integer" column="content" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="link" type="integer" column="link" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
