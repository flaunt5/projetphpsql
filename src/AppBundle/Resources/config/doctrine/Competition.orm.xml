<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Competition" table="competition">
    <indexes>
      <index name="clubId" columns="clubId"/>
      <index name="infrastructureId" columns="infrastructureId"/>
    </indexes>
    <id name="idcompetition" type="integer" column="idCompetition">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="startdate" type="date" column="startDate" nullable="true"/>
    <field name="enddate" type="date" column="endDate" nullable="false"/>
    <field name="clubid" type="integer" column="clubId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="signupprice" type="decimal" column="signupPrice" precision="10" scale="0" nullable="false"/>
    <field name="infrastructureid" type="integer" column="infrastructureId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
