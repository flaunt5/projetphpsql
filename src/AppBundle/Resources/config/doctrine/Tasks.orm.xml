<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Tasks" table="tasks">
    <indexes>
      <index name="infrastructureId" columns="infrastructureId"/>
      <index name="horseId" columns="horseId"/>
    </indexes>
    <id name="idtasks" type="integer" column="idTasks">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="desc" type="text" column="desc" length="16777215" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="infrastructureid" type="integer" column="infrastructureId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="horseid" type="integer" column="horseId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="frequency" type="integer" column="frequency" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
