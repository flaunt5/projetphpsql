<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Horsemodifierlist" table="horsemodifierlist">
    <indexes>
      <index name="horseId" columns="horseId"/>
      <index name="modifierId" columns="modifierId"/>
    </indexes>
    <id name="idhorsemodifierlist" type="integer" column="idHorseModifierList">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="horseid" type="integer" column="horseId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="modifierid" type="integer" column="modifierId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
