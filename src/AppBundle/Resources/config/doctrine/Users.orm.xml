<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="users">
    <indexes>
      <index name="bankId" columns="bankId"/>
    </indexes>
    <id name="iduser" type="integer" column="idUser">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="userName" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastlogin" type="datetime" column="lastLogin" nullable="true"/>
    <field name="typeuser" type="string" column="typeUser" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="firstname" type="string" column="firstName" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastName" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthdate" type="date" column="birthDate" nullable="true"/>
    <field name="phone" type="string" column="phone" length="18" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="avatar" type="string" column="avatar" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="desc" type="text" column="desc" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="website" type="string" column="website" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ip" type="string" column="ip" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="signupdate" type="date" column="signupDate" nullable="false"/>
    <field name="bankid" type="integer" column="bankId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
